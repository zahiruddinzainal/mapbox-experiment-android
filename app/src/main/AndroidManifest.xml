<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mapbox">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@drawable/default_app_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/default_app_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MapBoxPlanner"></activity>
        <activity android:name=".GetArray" />
        <activity android:name=".Cumulative">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MapBox2" />
        <activity android:name=".PickLocationDestination" />
        <activity android:name=".PickLocationOrigin" />
        <activity
            android:name=".MapBox"
            android:label="@string/app_name" />
    </application>

</manifest>